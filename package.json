{
  "name": "mystical-memoir",
  "version": "1.0.0",
  "description": "A magical mystical themed digital journal with enchanted elements, todo lists, drawing, stickers, and sharing capabilities",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:QAPournima/MysticalMemoir.git"
  },
    "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",  
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test-db": "node server/test-db.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "mystical",
    "memoir",
    "journal",
    "diary",
    "magical",
    "enchanted",
    "pwa",
    "todo",
    "drawing",
    "sparkles"
  ],
  "author": "QAPournima",
  "license": "MIT"
}
